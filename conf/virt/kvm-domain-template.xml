<domain type='kvm'>
<name>${name}</name>
<uuid>${uuid}</uuid>
<memory>${memSize}</memory>
<vcpu>${cpuCount}</vcpu>
<os>
  <type arch='${arch}' machine='pc-0.12'>hvm</type>
  <boot dev='${bootDevice}'/>
</os>
<features>
  <pae/>
  <acpi/>
</features>
<devices>
  <emulator>${emulatorPath}</emulator>
  <disk type='file' device='disk'>
    <driver name='qemu' type='qcow2'/>
    <source file='${sourceFile}'/>
    <target dev='hda'/>
  </disk>
  ${determinCdrom}
  ${determinNetwork}
  <graphics type='vnc' port='-1' listen='0.0.0.0'/>
  ${determinVnc}
</devices>
</domain>
