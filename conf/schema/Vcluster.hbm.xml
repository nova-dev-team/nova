<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


	<class name="nova.master.models.Vcluster" table="vcluster">
		<id name="id" column="id" type="long">
			<generator class="native"></generator>
		</id>
		<property name="clusterName" column="cluster_name" type="string"></property>
		<property name="fristIp" column="frist_ip" type="string"></property>
		<property name="clusterSize" column="cluster_size" type="integer"></property>
		<property name="userId" column="user_id" type="long"></property>
		<property name="sshPublicKey" column="ssh_public_key" type="string"></property>
		<property name="sshPrivateKey" column="ssh_private_key" type="string"></property>
		<property name="osUsername" column="os_username" type="string"></property>
		<property name="osPassword" column="os_password" type="string"></property>

	</class>
</hibernate-mapping>
