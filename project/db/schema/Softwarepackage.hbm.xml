<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


	<class name="nova.master.models.SoftwarePackage" table="softwarepackage">
		<id name="id" column="id" type="long">
			<generator class="native"></generator>
		</id>
		<property name="fileName" column="file_name" type="string"></property>
		<property name="displayName" column="display_name" type="string"></property>
		<property name="description" column="description" type="string"></property>
		<property name="osFamily" column="os_family" type="string"></property>
	</class>
</hibernate-mapping>