<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>


<class name="nova.master.models.Pnode1" table="pnode">  
	<id name="id" column="id" type="long">
		<generator class="native"></generator>
	</id>
        <property name="ip" column="ip" type="string"></property>
        <property name="status" column="status" type="string"></property>
        <property name="hostname" column="hostname" type="string"></property>
        <property name="uuid" column="uuid" type="string"></property>
        <property name="macAddress" column="mac_address" type="string"></property>
        <property name="vmCapacity" column="vm_capacity" type="integer"></property>
  
</class>
</hibernate-mapping>