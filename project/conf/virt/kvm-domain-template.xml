<domain type='kvm'>
<name>${name}</name>
<uuid>${uuid}</uuid>
<memory>${memSize}</memory>
<vcpu>${cpuCount}</vcpu>
<os>
  <type arch='${arch}' machine='pc'>hvm</type>
  <boot dev='${bootDevice}'/>
</os>
<features>
  <pae/>
  <acpi/>
</features>
<devices>
  <emulator>${emulatorPath}</emulator>
  <disk type='file' device='disk'>
    <source file='/var/lib/libvirt/images/linux.img'/>
    <target dev='hda'/>
  </disk>
  ${determinCdrom}
  ${determinNetwork}  
  <graphics type='vnc' port='-1' listen='0.0.0.0'/>
    ${determinVnc}
</devices>
</domain>
